#6
import random
import statistics

random_numbers = []

for i in range(1000):
  number = random.uniform(60, 70)
  random_numbers.append(number)

sample_means = []

for i in range(500):
  sample = random.sample(random_numbers, 30)
  sample_mean = statistics.mean(sample)
  sample_means.append(sample_mean)

print("Sample means:", sample_means)

